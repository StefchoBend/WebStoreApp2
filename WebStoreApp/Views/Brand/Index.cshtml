@model IEnumerable<WebStoreApp.Models.Brand.BrandIndexVM>
@{
    ViewData["Title"] = "Index";
}
<h1>All Brands</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BrandName)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.BrandName)
                </td>
            </tr>
        }
    </tbody>
</table>

